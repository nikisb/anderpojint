    if(PlayerInfo[playerid][pLevel] < 5) return SCM(playerid, -1, ""SIVA"Morate biti level 5 za davanje oglasa!");
	if(sscanf(params, "s[100]", result)) return SCM(playerid,-1,  ""SPLAVA"[V:RPG] Pomoc | "BELA"/oglasi [tekst]");
