	}
	PlayerInfo[playerid][pClan] = 0;
	PlayerInfo[playerid][pRank] = 0;
	PlayerInfo[playerid][pLider] = 0;
	PlayerInfo[playerid][pSkin] = 1;
	PlayerInfo[playerid][pRankName] = 0;
