	return true;
}

SacuvajUlicu( idulice ) 
{
	new q[301];
	mysql_format(SQL, q, sizeof(q), "UPDATE `ulice` SET `uUlicaX` = '%f', `uUlicaY` = '%f', `uUlicaZ` = '%f', `uPostavljena` = '%d', `uIme` = '%s' , `uText` = '%s', `uVlasnik` = '%s' WHERE `uSQLID` = '%d' LIMIT 1",
	UI[ idulice ][ uUlicaX ],
	UI[ idulice ][ uUlicaY ],
	UI[ idulice ][ uUlicaZ ],
	UI[ idulice ][ uPostavljena ],
	UI[ idulice ][ uIme ],
	UI[ idulice ][ uText ],
	UI[ idulice ][ uVlasnik ],
	UI[ idulice ][ uSQLID ]);
	mysql_tquery(SQL, q);
	return true;
}
