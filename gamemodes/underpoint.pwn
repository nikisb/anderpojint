			        SetWarStatsTD( w );
				}
				if( WARPInfo[ killerid ][ warTeam ][ 0 ] == true && WARPInfo[ playerid ][ warTeam ][ 1 ] == true ) {
			        WARInfo[ w ][ warTeamScore ][ 0 ]++;
