	new droga1[24],materijali1[24],oruzije[24],string[128];
	if(PlayerInfo[id][pDroga] > 0) droga1 = "Poseduje Drogu";
	else if(PlayerInfo[id][pDroga] == 0) droga1 = "Nema Droge!";
