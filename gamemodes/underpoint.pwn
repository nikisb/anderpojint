ResetWarStatistics( playerid ) {
    WARPInfo[ playerid ][ warTeam ][ 0 ] = false;
    WARPInfo[ playerid ][ warTeam ][ 1 ] = false;
    WARPInfo[ playerid ][ warKills ] = 0;
    WARPInfo[ playerid ][ warDeaths ] = 0;
