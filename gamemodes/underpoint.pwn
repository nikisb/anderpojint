	if(PlayerInfo[playerid][pLider] == 1 || PlayerInfo[playerid][pClan] == 1) { format(string, sizeof(string), ""SIVA"Novo Hapsenje | "CRVENA2"Policajac %s je uhapsio %s",GetName(playerid),GetName(id)); }
