    if(PlayerInfo[playerid][pLider] == 16 || PlayerInfo[playerid][pClan] == 16) { format(string, sizeof(string), ""SIVA"Novo Hapsenje | "CRVENA2"FBI %s je uhapsio %s",GetName(playerid),GetName(id)); }
