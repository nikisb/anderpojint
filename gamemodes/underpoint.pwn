			    DestroyDynamic3DTextLabel( LabelcicUlica[ i ] );
		        strmid( UI[ i ][ uIme ],ide, 0, strlen( ide ),40 );
				UlicaLabelIPickup(i);
				SacuvajUlicu(i);
