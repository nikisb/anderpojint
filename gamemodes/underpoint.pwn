CMD:smsoglas(playerid, params[])
{

    new result[512],string[256];
    if(PlayerInfo[playerid][pZatvor] > 0) return SCM(playerid, -1, "[V:RPG] "CRVENA"Ne mozete koristiti ovu komandu jer ste u zatvoru!");
