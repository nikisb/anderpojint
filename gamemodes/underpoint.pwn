	    if( killerid != INVALID_PLAYER_ID ) {
			if( WARInfo[ w ][ warActive ] == true && WARPInfo[ playerid ][ warID ] == w && WARPInfo[ killerid ][ warID ] == w ) {
				WARPInfo[ killerid ][ warKills ]++;
