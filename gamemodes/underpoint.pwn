    JI[Jelkaid][zZ],
    JI[Jelkaid][zAngle],
    JI[Jelkaid][aInt],
    JI[Jelkaid][aVW],

	JI[Jelkaid][jSQLID]);
	mysql_tquery(SQL, q);
    return 1;
}
