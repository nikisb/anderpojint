		}
		if(reason == 27)
		{
			if(PlayerInfo[killerid][pCombSkill] >= 0 && PlayerInfo[killerid][pCombSkill] <= 1000)
			{
			    PlayerInfo[killerid][pCombSkill] += 5;
			    SetPlayerSkills(killerid);
