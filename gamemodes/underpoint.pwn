SacuvajJelku(Jelkaid)
{   
    new q[256];
	mysql_format(SQL, q, sizeof(q), "UPDATE `jelke` SET `Postavljena` = '%d', `X` = '%f', `Y` = '%f', `Z` = '%f', `Angle` = '%s', `Interior` = '%d', `VW` = '%d' WHERE `jSQLID` = '%d' LIMIT 1",
	JI[Jelkaid][zPostavljena],
    JI[Jelkaid][zX],
