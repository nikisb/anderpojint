		}

		if(reason == 28 || reason == 32)
		{
			if(PlayerInfo[killerid][pUziSkill] >= 0 && PlayerInfo[killerid][pUziSkill] <= 800)
			{
			    PlayerInfo[killerid][pUziSkill] += 5;
