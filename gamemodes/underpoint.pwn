GetPlayerID(const Name[])
{
	foreach(new i:Player) {
		if(IsPlayerConnected(i) == 1) {
			new zzzName[MAX_PLAYER_NAME];
			GetPlayerName(i, zzzName, sizeof(zzzName));
			if(strcmp(Name, zzzName, true) == 0) 
			{
				return i;
			}
		}
	}
	return INVALID_PLAYER_ID;
}
