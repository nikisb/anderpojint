	else if(PlayerInfo[id][pMats] == 0) materijali1 = "Nema Materijala!";
	if(GetPlayerWeapon(id) > 0) oruzije = "Poseduje Oruzije";
	else if(GetPlayerWeapon(id) == 0) oruzije = "Nema Oruzije!";
