		    if(PlayerInfo[playerid][pAdmin] == 8) { aRank = "Developer"; }
			format(string, sizeof(string), ""VARADERO"[INFO] "BELA"%s %s vam je skinuo GameMastera.", aRank, GetName(playerid));
