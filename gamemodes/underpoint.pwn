  			SCMF(playerid,-1,""CRVENA"[V:RPG] | "BELA"Uspesno ste kupili polovan auto za %d$ novca!",AutoSalon[vid][1]/2);
  			UsaoAutoProdaja[playerid] = -1;
		}
		else return SCM(playerid,-1,""CRVENA"[V:RPG] | "SIVA"To vozilo nije na prodaju!");
    }
    return 1;
}

