	}
	if(PlayerInfo[playerid][pPosedovanjeAuta] != -1 && strcmp(GetName(playerid), VoziloInfo[vozilo1][vVlasnik], true) == 0)
	{
	    strmid(VoziloInfo[vozilo1][vVlasnik], name, 0, strlen(name), 24);
