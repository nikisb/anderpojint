	Update_Skin(playerid,PlayerInfo[playerid][pSkin]);
	SpawnIgraca(playerid);
	SpawnPlayer(playerid);
	SCM(playerid,-1,""SPLAVA"[V:RPG] "SIVA"Napustili ste vasu organizaciju!");
	SavePlayer(playerid);
	return 1;
}
