    format(string,sizeof(string),"[V:RPG] | "CRVENA"Firma Kreiran (Ovo je firma ID: %d)!",firmaid);
	SCM(playerid,-1,string);
	FirmaLabelIPickup(firmaid); 
	SacuvajFirmu(firmaid);


